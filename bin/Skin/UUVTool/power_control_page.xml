          <Option name="otherPowerOption" padding="5,3,2,0" selected="false" width="15" height="15" text="" readonly="true" normalimage="file='res/checkbox.png' source='0,0,15,15'" hotimage="file='res/checkbox.png' source='0,15,15,30'" selectedimage="file='res/checkbox.png' source='0,30,15,45'" selectedhotimage="file='res/checkbox.png' source='0,45,15,60'" />
